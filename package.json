{
  "scripts": {
    "upgrade-interactive": "npm-check --update",
    "composer:optimize-autoloader": "composer dump-autoload -a",
    "db-import:staging": "npm run-script db-pull:staging && npm run-script db-push:dev",
    "db-pull:staging": "node lib/bob db pull --host=\"#TASK_DB_HOST_STAGING\" --db=\"#TASK_DB_NAME_STAGING\" --user=\"#TASK_DB_USER_STAGING\" --password=\"#TASK_DB_PASS_STAGING\" db_dump/dev.sql",
    "db-push:dev": "node lib/bob db push --db=\"#DB_NAME\" db_dump/dev.sql",
    "watch:live": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack serve",
    "watch:es5:live": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack serve --env ecmaTarget=es5",
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack",
    "build:es5:dev": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack --env ecmaTarget=es5",
    "build:staging": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack --env env=staging",
    "build:es5:staging": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack --env ecmaTarget=es5 --env env=staging",
    "build:production": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack --env env=production",
    "build:es5:production": "cross-env TS_NODE_PROJECT=\"tsconfig.build.json\" npx webpack --env ecmaTarget=es5 --env env=production",
    "deploy:craft": "node lib/bob ftp --include=\"[\\\"config/**/*\\\", \\\"modules/**/*\\\", \\\"templates/**/*\\\", \\\"translations/**/*\\\", \\\"composer.*\\\"]\"",
    "deploy:webroot": "node lib/bob ftp --include=\"[\\\"web/assets/**/*\\\", \\\"web/*.*\\\", \\\"web/.htaccess\\\"]\""
  },
  "author": "BINARY one",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "gsap": "^3.6.1",
    "jquery": "^3.6.0",
    "slick-carousel": "^1.8.1",
    "tailwind-caret-color": "^1.1.0",
    "tailwindcss": "^2.1.1",
    "vue": "^2.6.11",
    "vue-agile": "^1.1.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.41",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.3",
    "@types/slick-carousel": "^1.6.34",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-env": "^1.16.0",
    "@types/webpack-manifest-plugin": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "core-js": "^3.10.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "dotenv": "^8.1.0",
    "eslint": "^7.24.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "ftp-deploy": "^2.4.1",
    "mini-css-extract-plugin": "^1.5.0",
    "minimatch": "^3.0.4",
