    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vue-axios": "^3.2.4",
    "vue-loader": "^15.9.6",
